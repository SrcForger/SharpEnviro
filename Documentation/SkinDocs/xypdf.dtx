% \subsection{Squiggled curves}
%
% \begin{macro}{\xP@splinesquiggled}
%    \begin{macrocode}
\newcommand*\xP@splinesquiggled{%
  \xP@checkspline\xP@splinesquiggled@\z@}
%    \end{macrocode}
% \end{macro}
% \begin{macro}{\xP@splinedblsquiggled}
