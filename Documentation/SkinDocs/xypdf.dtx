% a(0) **\dir{-},
% a(6.524)**\dir{-},
% a(13.048) **\dir{-},
% a(19.572) **\dir{-},
% a(26.096) **\dir{-},
% a(32.62) **\dir{-},
% a(39.144) **\dir{-},
% @+(-.2,.5),
% @+(.5,.3),
% a(70) *+<.5pt>{} **\crvs{-}
% \endxy}\endxy}
% \begin{figure}\label{Fig1}
