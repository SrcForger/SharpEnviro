<ClockSkins>
	<!-- Analog Clock //-->
	<ClockSkin>
		<Info>
			<Name>Elements</Name>
			<author>The SharpEnviro Team</author>
			<Website>http://www.sharpenviro.com/</Website>
			<Category>Analog</Category>
			<Filter>Big</Filter>
		</Info>
		<Skin>
			<Background>Background.png</Background>
			<Overlay>Overlay.png</Overlay>
			<HandHour>Hour.png</HandHour>
			<HandMinute>Minute.png</HandMinute>
			<HandSecond>Second.png</HandSecond>
		</Skin>
	</ClockSkin>
	
	<!-- Digial Clock //-->
	<ClockSkin>
		<Info>
			<Name>Elements</Name>
			<Author>The SharpEnviro Team</Author>
			<Website>http://www.sharpenviro.com/</Website>
			<Category>Digital</Category>
			<Filter>Medium</Filter>
		</Info>
		<Skin>
			<Background>Digital-Background.png</Background>
		</Skin>
		<Text>
			<Caption>{t}</Caption>
			<FontSize>28</FontSize>
			<FontColor>16777215</FontColor>
			<FontAlpha>255</FontAlpha>
			<ClearType>1</ClearType>
			<Align>1</Align>
			<X>110</X>
			<Y>5</Y>
		</Text>
		<Text>
			<Caption>{dddddd}</Caption>
			<FontSize>10</FontSize>
			<FontColor>16777215</FontColor>
			<FontAlpha>255</FontAlpha>
			<ClearType>0</ClearType>
			<Align>1</Align>
			<X>110</X>
			<Y>55</Y>
		</Text>
	</ClockSkin>
</ClockSkins>